<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- by Name Autowiring -->
	<bean id="human" class="com.autowire.Human" autowire="byName" />
	<!-- <bean id="heart" class="com.autowire.Heart"/> -->

	<!-- by Type Autowiring -->
	<bean id="humanfortype" class="com.autowire.Human" autowire="byType" />
<!--  	<bean id="heartfortype" class="com.autowire.Heart"/> -->

<!-- Contructor Autowiring -->
    <bean id="humanforContructorAutowiring" class="com.autowire.Human"
		autowire="constructor" />
	<bean id="heartforContructorAutowiring" class="com.autowire.Heart"/>
</beans>